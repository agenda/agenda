{
  "name": "agenda-rest",
  "version": "6.0.0-pre",
  "description": "REST API for Agenda job scheduling",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "agenda-rest": "./bin/agenda-rest.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc -b",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "node bin/agenda-rest.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/agenda/agenda"
  },
  "keywords": [
    "agenda",
    "rest",
    "api",
    "job",
    "scheduler"
  ],
  "license": "MIT",
  "dependencies": {
    "agenda": "workspace:*",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.4.0",
    "koa-router": "^13.0.0",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "@types/node": "^22.0.0",
    "mongodb": "^6.12.0",
    "mongodb-memory-server": "^10.0.0",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2",
    "vitest": "^3.1.0"
  }
}
