{
  "name": "agendash",
  "version": "6.0.0-alpha.0",
  "description": "Dashboard for Agenda job scheduler",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "public"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc -b && vite build --config client/vite.config.ts",
    "build:client": "vite build --config client/vite.config.ts",
    "dev": "tsx dev-server.ts",
    "dev:client": "vite --config client/vite.config.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/agenda/agenda"
  },
  "keywords": [
    "agenda",
    "dashboard",
    "job",
    "scheduler",
    "admin"
  ],
  "license": "MIT",
  "peerDependencies": {
    "agenda": "workspace:*"
  },
  "optionalDependencies": {
    "express": "^4.18.0 || ^5.0.0",
    "koa": "^2.14.0",
    "koa-router": "^12.0.0 || ^13.0.0",
    "koa-static": "^5.0.0",
    "koa-bodyparser": "^4.4.0",
    "fastify": "^4.0.0 || ^5.0.0",
    "@fastify/static": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "@hapi/hapi": "^21.0.0",
    "@hapi/inert": "^7.0.0"
  },
  "devDependencies": {
    "@agendajs/mongo-backend": "workspace:*",
    "@types/express": "^4.17.21",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-vue": "^5.2.0",
    "agenda": "workspace:*",
    "bootstrap": "^5.3.0",
    "date-fns": "^4.1.0",
    "express": "^4.21.2",
    "fastify": "^5.0.0",
    "@fastify/static": "^8.0.0",
    "@hapi/hapi": "^21.0.0",
    "@hapi/inert": "^7.0.0",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.4.0",
    "koa-router": "^13.0.0",
    "koa-static": "^5.0.0",
    "mongodb": "^6.12.0",
    "mongodb-memory-server": "^10.0.0",
    "supertest": "^7.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^3.1.0",
    "vue": "^3.5.0",
    "vue-tsc": "^2.2.0"
  }
}
