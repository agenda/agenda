{
  "name": "@agendajs/mongo-backend",
  "version": "3.1.0",
  "description": "MongoDB backend for Agenda job scheduler",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "import": "./dist/testing.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc -b && tsc -p tsconfig.test.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:debug": "DEBUG=agenda:**,-agenda:internal:** vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/agenda/agenda"
  },
  "keywords": [
    "agenda",
    "job",
    "scheduler",
    "mongodb",
    "mongo"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agenda/agenda/issues"
  },
  "dependencies": {
    "debug": "^4.4.0",
    "mongodb": "^6.12.0"
  },
  "peerDependencies": {
    "agenda": "workspace:*"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^3.1.0",
    "agenda": "workspace:*",
    "mongodb-memory-server": "^10.0.0",
    "tsx": "^4.21.0",
    "vitest": "^3.1.0"
  }
}
